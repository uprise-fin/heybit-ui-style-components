{
  "name": "heybit-ui-styled-components",
  "customElements": "custom-elements.json",
  "version": "0.12.12",
  "files": [
    "dist/**"
  ],
  "main": "dist/index.js",
  "keywords": [
    "front-end",
    "uprise-fin",
    "heybit",
    "ui"
  ],
  "homepage": "http://uprise-fin.github.io/heybit-ui-styled-components",
  "repository": "git://github.com/uprise-fin/heybit-ui-styled-components.git",
  "author": "uprise-fin",
  "license": "MIT",
  "jest": {
    "runner": "@jest-runner/electron",
    "testEnvironment": "@jest-runner/electron/environment",
    "preset": "ts-jest",
    "setupFiles": [
      "./dist/index.js"
    ],
    "collectCoverage": false,
    "testRegex": ".*\\.spec\\.ts$",
    "collectCoverageFrom": [
      "src/**/*.ts"
    ]
  },
  "scripts": {
    "svg": "node node/svg.js",
    "dev": "npx webpack serve --config webpack.dev.js",
    "build": "npx webpack build --config webpack.prod.js",
    "cem": "cem analyze",
    "test": "jest -i",
    "storybook": "start-storybook -p 6006",
    "pre-publish": "pnpm svg && pnpm cem && pnpm build && pnpm test",
    "build-storybook": "pnpm svg && build-storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.17.0",
    "@custom-elements-manifest/analyzer": "^0.5.7",
    "@jest-runner/electron": "^3.0.1",
    "@storybook/addon-actions": "^6.5.6",
    "@storybook/addon-essentials": "^6.5.6",
    "@storybook/addon-links": "^6.5.6",
    "@storybook/builder-webpack5": "^6.5.6",
    "@storybook/manager-webpack5": "^6.5.6",
    "@storybook/web-components": "^6.5.6",
    "@types/jest": "^27.4.0",
    "@types/react": "^18.0.8",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.5.1",
    "css-modules-typescript-loader": "^4.0.1",
    "dotenv": "^14.2.0",
    "electron": "^11.4.6",
    "extract-loader": "^5.1.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "26.0.1",
    "lit-scss-loader": "^1.1.0",
    "mini-css-extract-plugin": "^2.5.2",
    "pretendard": "^1.2.1",
    "sass": "^1.49.7",
    "sass-loader": "^12.4.0",
    "svg-inline-loader": "^0.8.2",
    "terser-webpack-plugin": "^5.3.1",
    "ts-jest": "^26.0.0",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.4",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.3",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@webcomponents/webcomponentsjs": "^2.6.0",
    "lit": "^2.1.1"
  },
  "optionalDependencies": {
    "lit-html": "^2.2.3",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "style-loader": "^3.3.1"
  }
}
