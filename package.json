{
  "name": "heybit-ui-styled-components",
  "customElements": "custom-elements.json",
  "version": "0.25.4",
  "files": [
    "dist/src/**"
  ],
  "main": "dist/src/index.js",
  "keywords": [
    "front-end",
    "uprise-fin",
    "heybit",
    "ui"
  ],
  "homepage": "http://uprise-fin.github.io/heybit-ui-styled-components",
  "repository": "git://github.com/uprise-fin/heybit-ui-styled-components.git",
  "author": "uprise-fin",
  "license": "MIT",
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint . --ext .ts --fix"
    ]
  },
  "jest": {
    "runner": "@jest-runner/electron",
    "testEnvironment": "@jest-runner/electron/environment",
    "preset": "ts-jest",
    "setupFiles": [
      "./dist/src/module.js"
    ],
    "moduleNameMapper": {
      "@/(.*)$": "<rootDir>/src/$1",
      "~/(.*)$": "<rootDir>/$1"
    },
    "collectCoverage": false,
    "testRegex": ".*\\.spec\\.ts$",
    "collectCoverageFrom": [
      "src/**/*.ts"
    ]
  },
  "scripts": {
    "svg": "node node/svg.js",
    "dev": "npx webpack serve --config webpack.dev.js",
    "build": "npx webpack build --config webpack.prod.js",
    "cem": "cem analyze",
    "test": "jest -i",
    "storybook": "pnpm env:dev && start-storybook -p 6006",
    "pre-publish": "pnpm env:dev && pnpm svg && pnpm cem && pnpm build && pnpm test",
    "build-storybook": "pnpm env:prod && pnpm svg && build-storybook",
    "env:dev": "cp ./env/.dev.env ./.env",
    "env:prod": "cp ./env/.prod.env ./.env",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/core": "^7.17.0",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@custom-elements-manifest/analyzer": "^0.5.7",
    "@jest-runner/electron": "^3.0.1",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/web-components": "^6.5.9",
    "@types/jest": "^27.4.0",
    "@types/react": "^18.0.8",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.5.1",
    "css-modules-typescript-loader": "^4.0.1",
    "dotenv": "^14.3.2",
    "dotenv-webpack": "^7.1.0",
    "electron": "^17.2.0",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.2.5",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.26.0",
    "extract-loader": "^5.1.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "jest": "26.0.1",
    "lint-staged": "^13.0.3",
    "lit-scss-loader": "^1.1.0",
    "mini-css-extract-plugin": "^2.5.2",
    "mrm": "^4.0.0",
    "pretendard": "^1.3.3",
    "prettier": "^2.7.1",
    "sass": "^1.49.7",
    "sass-loader": "^12.4.0",
    "storybook-color-picker": "^3.0.1",
    "style-loader": "^3.3.1",
    "svg-inline-loader": "^0.8.2",
    "terser-webpack-plugin": "^5.3.1",
    "ts-jest": "^26.0.0",
    "ts-loader": "^9.2.8",
    "ts-transform-paths": "^2.0.3",
    "typescript": "^4.5.4",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.3",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@webcomponents/webcomponentsjs": "^2.6.0",
    "lit": "^2.1.1",
    "matercolors": "^2.3.0"
  },
  "optionalDependencies": {
    "lit-html": "^2.2.3"
  }
}
